              "id": "anxiousScore",
              "label": "Anxious/Withdrawn (Clinical cut-off is 9.5)",
              "questionOptions": {
                "rendering": "text",
                "calculate": {
                  "calculateExpression": "(FORM.q1.score[q6] || 0) + (FORM.q1.score[q7] || 0) + (FORM.q1.score[q8] || 0) + (FORM.q1.score[q9] || 0) + (FORM.q1.score[q14] || 0) + (FORM.q1.score[q15] || 0) + (FORM.q1.score[q16] || 0)"
                }
              }
            },
            {
              "id": "fearfulScore",
              "label": "Fearful (Clinical cut-off is 8.5)",
              "questionOptions": {
                "rendering": "text",
                "calculate": {
                  "calculateExpression": "(FORM.q1.score[q3] || 0) + (FORM.q1.score[q4] || 0) + (FORM.q1.score[q5] || 0) + (FORM.q1.score[q6] || 0) + (FORM.q1.score[q10] || 0)"
                }
              }
            },
            {
              "id": "actingOutScore",
              "label": "Acting Out (Clinical cut-off is 13.5)",
              "questionOptions": {
                "rendering": "text",
                "calculate": {
                  "calculateExpression": "(FORM.q1.score[q1] || 0) + (FORM.q1.score[q2] || 0) + (FORM.q1.score[q11] || 0) + (FORM.q1.score[q12] || 0) + (FORM.q1.score[q13] || 0) + (FORM.q1.score[q17] || 0)"
                }
              }
            },
            {
              "id": "totalScore",
              "label": "Total Score",
              "questionOptions": {
                "rendering": "text",
                "disable": "true",
                "calculate": {
                  "calculateExpression": "anxiousScore + fearfulScore + actingOutScore + (FORM.q1.score[q18] || 0) + (FORM.q1.score[q19] || 0) + (FORM.q1.score[q20] || 0) + (FORM.q1.score[q21] || 0)"
                }
              }
            }
          ]
        }
